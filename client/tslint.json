{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "quotemark": false,
        "ordered-imports": false,
        "no-console": false,
        "array-type": false,
        "no-empty": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "max-classes-per-file": false,
        "variable-name": false,
        "interface-name": false,
        "no-empty-interface": false
    },
    "rulesDirectory": []
}
